class Solution {
    public void rotateArray(int[] nums, int k) {
        int n = nums.length;
        k = k % n;
        int[] result = new int[n];
        for(int i=0; i<n; i++) {
          result[i] = nums[(i + k) % n];
        }
         for(int i=0;i<n;i++){
            nums[i]=result[i];
         }
    }
}
